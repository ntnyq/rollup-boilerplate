{
  "name": "rollup-boilerplate",
  "version": "0.0.1",
  "packageManager": "pnpm@7.21.0",
  "description": "JavaScript library develop boilerplate based on rollup",
  "keywords": [
    "rollup",
    "library"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "ntnyq/rollup-boilerplate",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.min.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && rollup -c rollup.config.ts --configPlugin typescript",
    "clean": "rimraf dist",
    "coverage": "vitest --coverage",
    "lint": "eslint .",
    "prepare": "husky install",
    "release": "bumpp && npm publish",
    "test": "vitest"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.2.1",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/node": "^18.11.18",
    "@vitest/coverage-c8": "^0.26.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.31.0",
    "husky": "^8.0.2",
    "nano-staged": "^0.8.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vitest": "^0.26.2"
  },
  "nano-staged": {
    "*.{js,ts,md,json,yml,yaml,yaml}": "eslint --fix"
  },
  "engines": {
    "node": ">=14.18.0"
  }
}

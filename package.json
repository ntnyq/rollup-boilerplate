{
  "name": "foobar",
  "version": "0.0.1",
  "main": "dist/foobar.cjs.js",
  "module": "dist/foobar.esm.js",
  "unpkg": "dist/foobar.min.js",
  "keywords": [ "foo", "bar", "rollup" ],
  "author": "ntnyq <ntnyq13@gmail.com>",
  "repository": "ntnyq/rollup-boilterplate",
  "license": "MIT",
  "scripts": {
    "cz": "git-cz",
    "clean": "rimraf dist",
    "start": "npm run clean && cross-env NODE_ENV=development rollup -w -c scripts/rollup.config.dev.js",
    "build": "npm run clean && cross-env NODE_ENV=production rollup -c scripts/rollup.config.prod.js",
    "test": "jest",
    "serve": "serve -p 8080",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.js": [ "prettier --write", "eslint --fix", "git add" ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "files": [ "src", "dist/*.js" ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-eslint": "^10.0.1",
    "commitizen": "^3.0.5",
    "commitlint-config-cz": "^0.11.0",
    "cross-env": "^5.2.0",
    "cz-customizable": "^5.3.0",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "rollup": "^1.0.0",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^0.6.1",
    "rollup-plugin-uglify": "^6.0.0",
    "serve": "^10.1.1",
    "standard-version": "^4.4.0"
  }
}

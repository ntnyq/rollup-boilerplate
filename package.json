{
  "name": "rollup-boilerplate",
  "version": "0.0.1",
  "packageManager": "pnpm@8.2.0",
  "description": "JavaScript library develop boilerplate based on rollup",
  "keywords": [
    "rollup",
    "library"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "ntnyq/rollup-boilerplate",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "browser": "./dist/index.min.js",
  "unpkg": "./dist/index.min.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "run-s clean build-only",
    "build-only": "rollup -c rollup.config.mjs",
    "clean": "rimraf dist",
    "coverage": "vitest --coverage",
    "lint": "eslint .",
    "prepare": "husky install",
    "release": "bumpp && npm publish",
    "test": "vitest"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.6.1",
    "@ntnyq/prettier-config": "^1.6.1",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/node": "^18.15.11",
    "@vitest/coverage-c8": "^0.30.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "rollup": "^3.20.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,json,yml,yaml,yaml}": "eslint --fix"
  }
}
